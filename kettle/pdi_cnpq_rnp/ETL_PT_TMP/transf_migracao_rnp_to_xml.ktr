    <name>Table input - GT</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>db_rnp</connection>
    <sql>SELECT DISTINCT
  t1.id as id_gt
, t1.nome as nome_gt
, t1.sigla as sigla_gt
, t2.fase as fase
, t2.anoInicio as ano_inicio
, t2.anoFim as ano_fim
, t3.nome as nome_instituicao
, t3.sigla as sigla_instituicao
FROM gt t1, fase t2, instituicao t3, instituicaoemgt t4
WHERE t2.idGT = t1.id
AND t4.idGT = t1.id
AND t4.idInstituicao = t3.id
ORDER BY t1.id, t2.fase
</sql>
    <limit>0</limit>
    <lookup/>
