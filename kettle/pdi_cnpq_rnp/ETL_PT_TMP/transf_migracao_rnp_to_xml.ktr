    <sql>SELECT DISTINCT
  t1.id as id_pesquisador
, t1.nome_completo as nome_pesquisador
, t5.id as id_gt
, t5.nome as nome_gt
, t5.sigla as sigla_gt
, t4.fase as fase
, t3.tipo as tipo_participacao
FROM pesquisador t1, participacaoemgt t2, tipoparticipacaoemgt t3, fase t4, gt t5
WHERE t2.idPesquisador = t1.id
AND  t2.idFase = t4.id
AND  t2.idTipoParticipacao = t3.id
AND  t4.idGT = t5.id
ORDER BY t1.nome_completo, t5.nome, t4.fase, t3.tipo</sql>
